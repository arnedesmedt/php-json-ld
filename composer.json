{
    "name": "arnedesmedt/php-json-ld",
    "description": "Json LD for php",
    "type": "library",
    "homepage": "https://github.com/arnedesmedt",
    "license": "MIT",
    "authors": [
        {
            "name": "Arne De Smedt",
            "email": "arnedesmdt@gmail.com",
            "homepage": "https://github.com/arnedesmedt"
        }
    ],
    "require": {
        "php": "^7.3",
        "arnedesmedt/event-engine-immutable-objects": "dev-master"
    },
    "require-dev": {
        "phpro/grumphp": "^0.16.2",
        "phpstan/phpstan": "^0.11.19",
        "squizlabs/php_codesniffer": "^3.5",
        "doctrine/coding-standard": "^6.0",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "ADS\\JsonLD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ADS\\JsonLDTest\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
